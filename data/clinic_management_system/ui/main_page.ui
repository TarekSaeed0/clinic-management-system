<?xml version="1.0" encoding="UTF-8"?>
<!-- Created with Cambalache 0.94.0 --><interface>
	<requires lib="gtk" version="4.11"></requires>
	<template class="MainPage" parent="GtkBox">
		<property name="halign">center</property>
		<property name="orientation">vertical</property>
		<property name="spacing">10</property>
		<property name="valign">center</property>
		<child>
			<object class="GtkFrame">
				<property name="halign">center</property>
				<property name="margin-top">20</property>
				<property name="valign">center</property>
				<child>
					<object class="GtkBox">
						<property name="orientation">vertical</property>
						<property name="margin-bottom">25</property>
						<property name="margin-end">25</property>
						<property name="margin-start">25</property>
						<property name="margin-top">5</property>
						<child>
							<object class="GtkImage" id="logo">
								<property name="resource">/com/github/TarekSaeed0/clinic_management_system/images/logo.png</property>
								<property name="pixel-size">150</property>
							</object>
						</child>
						<child>
							<object class="GtkLabel">
								<property name="justify">center</property>
								<property name="label">Clinic Management System</property>
								<attributes>
									<attribute name="weight" value="bold"></attribute>
									<attribute name="scale" value="1.75"></attribute>
								</attributes>
							</object>
						</child>
					</object>
				</child>
			</object>
		</child>
		<child>
			<object class="GtkFrame">
				<property name="halign">center</property>
				<property name="margin-bottom">20</property>
				<property name="margin-top">20</property>
				<property name="valign">center</property>
				<child>
					<object class="GtkBox">
						<property name="halign">center</property>
						<property name="margin-bottom">15</property>
						<property name="margin-end">30</property>
						<property name="margin-start">30</property>
						<property name="margin-top">15</property>
						<property name="orientation">vertical</property>
						<property name="spacing">10</property>
						<property name="valign">center</property>
						<child>
							<object class="GtkButton" id="list_doctors_button">
								<property name="label">List Doctors</property>
								<signal handler="list_doctors_button_callback" name="clicked"></signal>
							</object>
						</child>
						<child>
							<object class="GtkButton" id="search_doctors_button">
								<property name="label">Search Doctors</property>
								<signal handler="search_doctors_button_callback" name="clicked"></signal>
							</object>
						</child>
						<child>
							<object class="GtkButton" id="sign_out_button">
								<property name="label">Sign Out</property>
								<signal handler="sign_out_button_callback" name="clicked"></signal>
							</object>
						</child>
					</object>
				</child>
			</object>
		</child>
	</template>
</interface>
